<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="windowActionModeOverlay">true</item>
    </style>
    <style name="ZeroButton" parent="ThemeOverlay.AppCompat.Dark">
        <item name="colorButtonNormal">@color/color_dark_green</item>

    </style>
    <style name="FiveButton" parent="Theme.AppCompat.Light">
        <item name="colorButtonNormal">@color/color_green</item>

    </style>
    <style name="TwelveButton" parent="Theme.AppCompat.Light">
        <item name="colorButtonNormal">@color/color_light_green</item>

    </style>
    <style name="EighteenButton" parent="Theme.AppCompat.Light">
        <item name="colorButtonNormal">@color/color_orange</item>

    </style>
    <style name="TwentyEightButton" parent="ThemeOverlay.AppCompat.Dark">
        <item name="colorButtonNormal">@color/color_red</item>

    </style>
    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.NoActionBar.Fullscreen" parent="AppTheme.NoActionBar">
        <item name="android:windowFullscreen">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>
